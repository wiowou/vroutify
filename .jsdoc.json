{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+\\.mjs(doc|x)?$",
    "excludePattern": "README.md"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
